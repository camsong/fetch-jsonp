{
  "name": "fetch-jsonp",
  "version": "1.1.2",
  "description": "Fetch JSONP like a boss using Fetch API",
  "main": "build/fetch-jsonp.js",
  "scripts": {
    "prepublish": "npm run lint && npm run clean && npm run build",
